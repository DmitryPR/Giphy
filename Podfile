source 'https://github.com/CocoaPods/Specs.git'

platform:ios, :deployment_target => '8.0'

# ignore all warnings from all pods
inhibit_all_warnings!

# Swift and objc libraries together
use_frameworks!

target 'GiphyProject' do
    pod 'Alamofire', '~> 3.4'
	pod 'XCGLogger', '~> 3.3'
	pod 'SwiftyJSON'
	pod "PKHUD"
	pod 'SDWebImage'
end